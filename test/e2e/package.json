{
  "name": "mcp-cpp-e2e-tests",
  "version": "1.0.0",
  "type": "module",
  "description": "End-to-end testing framework for C++ MCP server",
  "scripts": {
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:framework": "vitest src/framework/__tests__",
    "test:e2e": "vitest src/tests",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "validate": "npm run lint && npm run format:check && npm run test:framework && npm run test:e2e"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.0.10",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "ajv": "^8.17.1",
    "eslint": "^9.30.1",
    "fs-extra": "^11.3.0",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.15.0"
  }
}
